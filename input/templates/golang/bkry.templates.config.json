[
    {
        "path": "config/**",
        "output_dir": "{ServerDir}/models/entities/{EntityName}/",
        "data": "single_entity"
    },
    {
        "path": "controllers/**",
        "output_dir": "{ServerDir}/transport/controllers/{EntityName}/",
        "data": "single_entity"
    },
    {
        "path": "operations/**",
        "output_dir": "{ServerDir}/transport/operations/{EntityName}/",
        "data": "single_entity"
    },
    {
        "path": "repository/repository.go.tmpl",
        "output_dir": "{ServerDir}/database/repository/entities/",
        "output_file": "{EntityName}.gen.go",
        "force_write": true,
        "data": "single_entity"
    },
    {
        "path": "repository/repo_interface.go.tmpl",
        "output_dir": "{ServerDir}/database/repository/entities/",
        "output_file": "index.gen.go",
        "force_write": true,
        "data": "all_entities"
    },
    {
        "path": "routes/**",
        "output_dir": "{ServerDir}/transport/routes/{EntityName}/",
        "data": "single_entity"
    },
    {
        "path": "api.go.tmpl",
        "output_dir": "{ServerDir}/transport",
        "output_file": "api.gen.go",
        "data": "all_entities",
        "force_write": true
    }
]