import { type EntityName, type EntityTypeName } from './types.gen.d';

export const entityCollectionNameToEntityName: Record<string, EntityName> = Object.freeze({
{{- range .Entities}}
  {{.CollectionName}}: '{{.EntityName}}',
{{- end}}
})

export const entityNameToTypeName: Record<EntityName, EntityTypeName> = Object.freeze({
{{- range .Entities}}
  {{.EntityName}}: '{{.TypeName}}',
{{- end}}
})

export const entityTypeName: Record<EntityTypeName, EntityTypeName> = Object.freeze({
{{- range .Entities}}
  {{.TypeName}}: '{{.TypeName}}',
{{- end}}
})